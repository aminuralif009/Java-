import java.util.Scanner;

public class App {
    public static void main(String[] args) throws Exception {
        Scanner a = new Scanner(System.in);
        
       int x = a.nextInt();
       if(x%2 == 0){
        System.out.println("Even\n");
       }else{
        System.out.println("Odd\n");
       }

        a.close();
    }
}
